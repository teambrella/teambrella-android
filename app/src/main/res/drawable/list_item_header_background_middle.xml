<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/paleGreyFour" />
        </shape>
    </item>

    <item android:gravity="bottom">
        <nine-patch
            android:dither="true"
            android:src="@drawable/list_delim_top" />
    </item>

    <item android:gravity="top">
        <nine-patch
            android:dither="true"
            android:src="@drawable/list_delim_bottom" />
    </item>

</layer-list>